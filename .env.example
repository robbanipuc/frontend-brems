# Backend API base URL (required for login and all API calls)
# When running frontend locally (e.g. npm run dev on port 5173), set this to your Laravel backend URL.
# Example: Laravel running on http://localhost:8000
VITE_API_URL=http://localhost:8000/api

# Cloudinary cloud name (for building file URLs). Get from Cloudinary dashboard.
VITE_CLOUDINARY_CLOUD_NAME=
